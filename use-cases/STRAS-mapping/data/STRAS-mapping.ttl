prima-core:Project a owl:Class ;
    rdfs:comment "Project is an enterprise (potentially individual but typically collaborative) of one or more research users, planned to perform one or more studies." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAttributedTo ;
            owl:someValuesFrom prima-core:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prima-core:hasStudy ;
            owl:someValuesFrom prima-core:Study ],
        prov:Entity ;
    prima-core:MDMCNEPGlossaryDefinition "Project is an enterprise (potentially individual but typically collaborative) of one or more research users, planned to perform one or more studies."@en .

prima-core:hasSetting a owl:ObjectProperty ;
    rdfs:label "has setting"@en ;
    rdfs:comment "hasSetting represents the relationship between equipment or research software to setting"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( prima-core:Equipment prima-core:ResearchSoftware ) ] ;
    rdfs:range prima-core:Setting .

prima-core:hasTechnique a owl:ObjectProperty ;
    rdfs:label "has technique"@en ;
    rdfs:comment "hasTechnique represents the relationship between data acquisition or data analysis lifecycle to technique"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( prima-core:DataAcquisition prima-core:DataAnalysisLifecycle ) ] ;
    rdfs:range prima-core:Technique .

prima-core:performsAgentRole a owl:ObjectProperty ;
    rdfs:label "performs agent role"@en ;
    rdfs:comment "performsAgentRole relates prov:Agent with AgentRole"@en ;
    rdfs:domain prov:Agent ;
    rdfs:range prima-core:AgentRole .

prima-dataset:DataCollaborationPlatform a owl:Class ;
    rdfs:comment "Data collaboration platform is an operational information system which allows research users to keep their research data, datasets and related documents (e.g., drafts of scientific publications) synchronized and up to date, and to exchange them with other research users, who are typically members of the same project." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prima-dataset:stores ;
            owl:someValuesFrom prima-dataset:Dataset ] ;
    prima-core:MDMCNEPGlossaryDefinition "Operational information system which allows research users to keep their research data, datasets and related documents (e.g., drafts of scientific publications) synchronized and up to date, and to exchange them with other research users, who are typically members of the same project. The system is intended for the long-tail and still volatile data, which can change and are still subject to active research. Therefore, a data collaboration platform offers versioning of all ingested files but does not usually assign persistent identifiers to them."@en .

prima-dataset:MetadataRepository a owl:Class ;
    rdfs:comment "Metadata repository is an information system used to store, manage and provide access to metadata, following a policy or a set of rules that define storage and access norms."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prima-dataset:stores ;
            owl:someValuesFrom prima-dataset:Metadata ] ;
    prima-core:MDMCNEPGlossaryDefinition "Information system used to store, manage and provide access to metadata, following a policy or a set of rules that define storage and access norms. Metadata repositories may be associated with an institution or a group of them, or may be run by a third party. Metadata repositories may or may not be directly used by research users."@en .

prima-dataset:PublicationData a owl:Class ;
    rdfs:comment "Dataset(s) generated in the course of a study, that has undergone quality assessment and can be referred to as citations (i.e., a persistent identifier is assigned to it), e.g., to validate the results and/or the conclusions presented in a scientific publication or appearing in it."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasDerivedFrom ;
            owl:someValuesFrom prima-dataset:Dataset ],
        [ a owl:Restriction ;
            owl:onProperty prima-dataset:hasMetadata ;
            owl:someValuesFrom prima-dataset:Metadata ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAttributedTo ;
            owl:someValuesFrom prima-core:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prima-dataset:isStoredIn ;
            owl:someValuesFrom prima-dataset:DataRepository ],
        pmd:DigitalEntity ;
    prima-core:MDMCNEPGlossaryDefinition "Dataset(s) generated in the course of a Study, that has undergone quality assessment and can be referred to as citations (i.e., a Persistent Identifier is assigned to it), e.g., to validate the results and/or the Conclusions presented in a Scientific Publication or appearing in it. Publication Data may include any kind of Research Data, as well as the relevant Metadata about the actions performed. Publication Data may be attributed to some or to all the Research Users who are members of the Project."@en .

prima-exp:isCarriedBy a owl:ObjectProperty ;
    rdfs:label "is carried by"@en ;
    rdfs:comment "isCarriedBy is the inverse property of carries."@en .

prima-exp:isHeldBy a owl:ObjectProperty ;
    rdfs:label "is held by"@en ;
    rdfs:comment "isHeldBy is the inverse property of holds."@en .

prima-exp:isSampleComponentOf a owl:ObjectProperty ;
    rdfs:label "is sample component of"@en ;
    rdfs:comment "isSampleComponentOf is the inverse property of hasSampleComponent."@en ;
    rdfs:subPropertyOf owl:topObjectProperty .

pmd:nextProcess a owl:ObjectProperty ;
    rdfs:domain pmd:Process ;
    rdfs:range pmd:Process ;
    rdfs:subPropertyOf prov:wasInformedBy ;
    owl:inverseOf pmd:previousProcess .

pmd:participant a owl:ObjectProperty ;
    rdfs:domain pmd:Process ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( pmd:DigitalEntity pmd:Object pmd:ValueObject ) ],
        [ a owl:Class ;
            owl:unionOf ( pmd:DigitalEntity pmd:Object pmd:ValueObject ) ] ;
    owl:inverseOf pmd:participantOf .

pmd:subordinateProcess a owl:ObjectProperty ;
    rdfs:domain pmd:Process ;
    rdfs:range pmd:Process .

