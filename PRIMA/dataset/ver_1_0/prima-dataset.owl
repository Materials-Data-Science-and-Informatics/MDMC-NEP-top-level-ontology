            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/dataset#isStoredIn"/>
