        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/dataset#PersistentIdentifier"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <rdfs:comment xml:lang="en">The scheme or type of the persistent identifier. Common values include: &quot;DOI&quot;, &quot;ARK&quot;, &quot;Handle&quot;, &quot;PURL&quot;, &quot;ORCID&quot;, &quot;ROR&quot;, &quot;ISNI&quot;, &quot;IGSN&quot;, &quot;LSID&quot;, &quot;URN&quot;, &quot;URI&quot;.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/prima/dataset"/>
        <rdfs:label xml:lang="en">identifier scheme</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/dataset#identifierValue -->

    <owl:DatatypeProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/dataset#identifierValue">
