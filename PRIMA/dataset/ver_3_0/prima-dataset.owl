        <dcterms:title xml:lang="en">PRIMA Ontology - Dataset Module</dcterms:title>
        <vann:preferredNamespacePrefix xml:lang="en">PRIMA</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">This is the dataset module of the Provenance Information for Materials Science (PRIMA) Ontology, version 3.0, aligned with PMDco v3 and based on BFO (Basic Formal Ontology).</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://github.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology"/>
        <owl:priorVersion rdf:resource="https://purls.helmholtz-metadaten.de/prima/dataset?version=2.0"/>
