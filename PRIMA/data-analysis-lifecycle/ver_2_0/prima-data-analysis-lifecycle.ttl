@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

<http://qudt.org/schema/qudt/numericValue> a owl:DatatypeProperty .

<http://qudt.org/schema/qudt/value> a owl:DatatypeProperty .

prov:endedAtTime a owl:DatatypeProperty .

prov:startedAtTime a owl:DatatypeProperty .

<https://purls.helmholtz-metadaten.de/prima/core#MDMCNEPGlossaryDefinition> a owl:AnnotationProperty .

<https://purls.helmholtz-metadaten.de/prima/dal> a owl:Ontology ;
    dcterms:contributor "Elda Osmenaj"@en,
        "Mirco Panighel"@en ;
    dcterms:created "2024-01-29"@en ;
    dcterms:creator "Ahmad Zainul Ihsan"@en,
        "Mehrdad Jalali"@en,
        "Rossella Aversa"@en ;
    dcterms:license <http://creativecommons.org/licenses/by/3.0/> ;
    dcterms:title "PRIMA Ontology- Data Analysis Lifecycle Module"@en ;
    vann:preferredNamespacePrefix "PRIMA"@en ;
    rdfs:comment "This is the data analysis lifecycle module of the Provenance Information for Materials Science (PRIMA) Ontology"@en ;
    rdfs:seeAlso <https://github.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology> ;
    owl:versionInfo "2.0"@en .

<https://purls.helmholtz-metadaten.de/prima/dal#MDMCNEPGlossaryDefinition> a owl:AnnotationProperty ;
    rdfs:subPropertyOf skos:definition .

<https://purls.helmholtz-metadaten.de/prima/dal#hasDataAnalysis> a owl:ObjectProperty ;
    rdfs:label "has data analysis"@en ;
    rdfs:comment "hasDataAnalysis relates DataAnalysisLifecycle with DataAnalysis"@en ;
    rdfs:domain <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> ;
    rdfs:range <https://purls.helmholtz-metadaten.de/prima/dal#DataAnalysis> ;
    owl:inverseOf <https://purls.helmholtz-metadaten.de/prima/dal#isDataAnalysisOf> .

<https://purls.helmholtz-metadaten.de/prima/dal#hasDataInterpretation> a owl:ObjectProperty ;
    rdfs:label "has data interpretation"@en ;
    rdfs:comment "hasDataInterpretation relates DataAnalysisLifecycle with DataInterpretation"@en ;
    rdfs:domain <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> ;
    rdfs:range <https://purls.helmholtz-metadaten.de/prima/dal#DataInterpretation> ;
    owl:inverseOf <https://purls.helmholtz-metadaten.de/prima/dal#isDataRepresentationOf> .

<https://purls.helmholtz-metadaten.de/prima/dal#hasDataProcessing> a owl:ObjectProperty ;
    rdfs:label "has data processing"@en ;
    rdfs:comment "hasDataProcessing relates DataAnalysisLifecycle with DataProcessing"@en ;
    rdfs:domain <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> ;
    rdfs:range <https://purls.helmholtz-metadaten.de/prima/dal#DataProcessing> ;
    owl:inverseOf <https://purls.helmholtz-metadaten.de/prima/dal#isDataProcessingOf> .

<https://w3id.org/pmd/co/composes> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Object> ;
    rdfs:range <https://w3id.org/pmd/co/Object> .

<http://qudt.org/schema/qudt/Quantity> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/QuantityKind> ;
            owl:onProperty <http://qudt.org/schema/qudt/hasQuantityKind> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/QuantityValue> ;
            owl:onProperty <http://qudt.org/schema/qudt/quantityValue> ] .

<http://qudt.org/schema/qudt/QuantityKind> a owl:Class .

<http://qudt.org/schema/qudt/QuantityValue> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/Unit> ;
            owl:onProperty <http://qudt.org/schema/qudt/unit> ] .

<http://qudt.org/schema/qudt/Unit> a owl:Class .

<http://qudt.org/schema/qudt/hasQuantityKind> a owl:ObjectProperty .

<http://qudt.org/schema/qudt/quantityValue> a owl:ObjectProperty .

<http://qudt.org/schema/qudt/unit> a owl:ObjectProperty .

skos:definition a owl:AnnotationProperty .

prov:Activity a owl:Class .

prov:SoftwareAgent a owl:Class ;
    rdfs:subClassOf prov:Agent .

prov:used a owl:ObjectProperty .

prov:wasAttributedTo a owl:ObjectProperty .

prov:wasInformedBy a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/core#Study> a owl:Class ;
    rdfs:comment "Set of all the processes and activities performed by one or more Research Users, who are part of the same Project, with the purpose of verifying, falsifying, or establishing the validity of a hypothesis and supporting the answer to some scientific research question. The output of a Study is usually reported in one or more Scientific Publications and may be supplemented by Publication Data."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/study> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/dal#hasConclusion> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dal#Conclusion> ],
        prov:Entity ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/study> .

<https://purls.helmholtz-metadaten.de/prima/dal#hasConclusion> a owl:ObjectProperty ;
    rdfs:label "has conclusion"@en ;
    rdfs:comment "hasConclusion represents the relationship between study to conclusion"@en ;
    rdfs:domain <https://purls.helmholtz-metadaten.de/prima/core#Study> ;
    rdfs:range <https://purls.helmholtz-metadaten.de/prima/dal#Conclusion> .

<https://purls.helmholtz-metadaten.de/prima/dal#isDataAnalysisOf> a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dal#isDataProcessingOf> a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dal#isDataRepresentationOf> a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dal#isUsedByResearchSoftware> a owl:ObjectProperty .

<https://w3id.org/pmd/co/characteristicOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/inputOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/nextProcess> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range <https://w3id.org/pmd/co/Process> ;
    rdfs:subPropertyOf prov:wasInformedBy ;
    owl:inverseOf <https://w3id.org/pmd/co/previousProcess> .

<https://w3id.org/pmd/co/outputOf> a owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty .

<https://w3id.org/pmd/co/participantOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/previousProcess> a owl:ObjectProperty .

<https://w3id.org/pmd/co/subordinateProcess> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range <https://w3id.org/pmd/co/Process> .

prov:Agent a owl:Class .

<https://w3id.org/pmd/co/DigitalEntity> a owl:Class ;
    rdfs:subClassOf prov:Entity .

<https://w3id.org/pmd/co/characteristic> a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( <https://w3id.org/pmd/co/Object> <https://w3id.org/pmd/co/Process> <https://w3id.org/pmd/co/ValueObject> ) ] ;
    rdfs:range <https://w3id.org/pmd/co/ValueObject> ;
    owl:inverseOf <https://w3id.org/pmd/co/characteristicOf> .

<https://w3id.org/pmd/co/participant> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( <https://w3id.org/pmd/co/DigitalEntity> <https://w3id.org/pmd/co/Object> <https://w3id.org/pmd/co/ValueObject> ) ] ;
    owl:inverseOf <https://w3id.org/pmd/co/participantOf> .

dcterms:references a owl:ObjectProperty .

prov:wasDerivedFrom a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dal#Conclusion> a owl:Class ;
    rdfs:comment "Conclusion is the primary output of data interpretation performed by one or more research users on research data, typically on analysed data, possibly using research software."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty dcterms:references ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ReferenceData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dal#DataInterpretation> ],
        prov:Entity .

<https://purls.helmholtz-metadaten.de/prima/dal#DataAnalysis> a owl:Class ;
    rdfs:comment "Set of actions performed by one or more Research Users, possibly using a Technique and Research Software, on Research Data, typically Processed Data, to extract insights that support the answer to some scientific research question. Data Analysis may include: linear combination fitting, least-squares curve fitting, data modelling, pattern extraction and/or segmentation, data visualization. The output of Data Analysis is Analyzed Data."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/dataanalysis> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/core#usesResearchSoftware> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#AnalyzedData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> ],
        <https://w3id.org/pmd/co/Process> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/dataanalysis> .

<https://purls.helmholtz-metadaten.de/prima/dal#DataInterpretation> a owl:Class ;
    rdfs:comment "Set of actions, included in the data analysis lifecycle, performed by one or more research users on research data, typically analyzed data, to determine the conclusions of the study, possibly in comparison with reference data."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#AnalyzedData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dal#Conclusion> ],
        [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/core#usesResearchSoftware> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> ],
        <https://w3id.org/pmd/co/Process> .

<https://purls.helmholtz-metadaten.de/prima/dal#DataProcessing> a owl:Class ;
    rdfs:comment "Set of actions performed by one or more Research Users, possibly using a Technique and Research Software, on Research Data, typically Reference Data or Raw Data, to prepare it for one or more further processes, e.g., Model Preparation, computational Data Acquisition, and/or Data Analysis. Data Processing usually consists of routine actions. It may include: filtering, denoising, transformation, fusion and/or compression of Research Data, as well as calibration, normalization, statistical data reduction, background subtraction and/or correction of artifacts. The output of Data Processing is Processed Data."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/dataprocessing> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/core#usesResearchSoftware> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ReferenceData> ],
        <https://w3id.org/pmd/co/Process> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/dataprocessing> .

<https://purls.helmholtz-metadaten.de/prima/dataset#AnalyzedData> a owl:Class ;
    rdfs:comment "Specific type of Research Data, primary output of any kind of Data Analysis performed on Research Data, typically on Processed Data."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/analyzeddata> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dal#DataAnalysis> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasDerivedFrom ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasDerivedFrom ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty dcterms:references ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ReferenceData> ],
        <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/analyzeddata> .

<https://purls.helmholtz-metadaten.de/prima/dataset#RawData> a owl:Class ;
    rdfs:comment "Specific type of Research Data, primary output of a Data Acquisition performed on a System, before any subsequent Data Processing."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata> ;
    rdfs:subClassOf <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata> .

prov:wasAssociatedWith a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/core#usesResearchSoftware> a owl:ObjectProperty ;
    rdfs:label "uses research sofware"@en ;
    rdfs:comment "usesResearchSoftware relates data analysis, data analysis lifecycle, data interpretation, or data processing with research software"@en ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> <https://purls.helmholtz-metadaten.de/prima/dal#DataAnalysis> <https://purls.helmholtz-metadaten.de/prima/dal#DataInterpretation> <https://purls.helmholtz-metadaten.de/prima/dal#DataProcessing> ) ] ;
    rdfs:range <https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> ;
    owl:inverseOf <https://purls.helmholtz-metadaten.de/prima/dal#isUsedByResearchSoftware> .

<https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> a owl:Class ;
    rdfs:comment "Specific type of Research Data, primary output of any kind of Data Processing performed on Research Data (typically, but not limited to, Raw Data or Reference Data). Processed Data is usually an intermediate result, to be used as input of one or more further processes, e.g., Model Preparation or Data Analysis."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/processeddata> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty dcterms:references ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ReferenceData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dal#DataProcessing> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasDerivedFrom ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> ],
        <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/processeddata> .

<https://purls.helmholtz-metadaten.de/prima/dataset#ReferenceData> a owl:Class ;
    rdfs:comment "Any Research Data from prior Studies or from literature, which is used in the course of a Study to, e.g., define a Model, compare Analyzed Data, and/or validate findings and results. Reference Data is typically retrieved from publicly accessible Data Repositories. Depending on the research context, Reference Data usually undergo some kind of Data Processing before being used."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/referencedata> ;
    rdfs:subClassOf <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/referencedata> .

prov:Entity a owl:Class .

prov:wasGeneratedBy a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> a owl:Class ;
    rdfs:comment "Any software used to process, analyze, or visualize Research Data (including data rendering and or plotting)."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/researchsoftware> ;
    rdfs:subClassOf prov:SoftwareAgent ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/researchsoftware> .

<https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> a owl:Class ;
    rdfs:comment "Person, usually member of a Project, who conducts any part of the Study to collect and/or to analyze Research Data, and/or reuses Reference Data to extract insights that support the answer to some specific research question, usually reported in one or more Scientific Publications. A Research User may be assigned with a User Role."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/researchuser> ;
    rdfs:subClassOf prov:Agent ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/researchuser> .

<https://w3id.org/pmd/co/ValueObject> a owl:Class ;
    rdfs:subClassOf <http://qudt.org/schema/qudt/Quantity>,
        prov:Entity .

<https://purls.helmholtz-metadaten.de/prima/core#DataAnalysisLifecycle> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#AnalyzedData> ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> ],
        [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/core#usesResearchSoftware> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchSoftware> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        <https://w3id.org/pmd/co/Process> .

<https://w3id.org/pmd/co/Object> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/characteristic> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/ValueObject> ],
        prov:Entity .

<https://w3id.org/pmd/co/output> a owl:ObjectProperty ;
    rdfs:subPropertyOf <https://w3id.org/pmd/co/participant> ;
    owl:inverseOf <https://w3id.org/pmd/co/outputOf> .

<https://purls.helmholtz-metadaten.de/prima/core#ResearchData> a owl:Class ;
    rdfs:comment "Data collected, created, or examined by one or more Research Users to be analyzed or considered as a basis for reasoning, discussion or calculation in a research context, with the purpose of generating, verifying, and/or validating original scientific claims that support the answer to some specific research question. According to this definition, Raw Data, Processed Data, Analyzed Data and Reference Data are particular types of Research Data. Research Data is typically in the form of one or more data files, but it may potentially be a data stream or any other form of data which is relevant in a particular data management context. Research Data may be described by Metadata and may be stored in a Data Collaboration Platform and/or in a Data Repository. Research Data may undergo some kind of Data Curation and be aggregated into a Dataset."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/researchdata> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAttributedTo ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/core#ResearchUser> ],
        <https://w3id.org/pmd/co/DigitalEntity> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/researchdata> .

<https://w3id.org/pmd/co/input> a owl:ObjectProperty ;
    rdfs:subPropertyOf <https://w3id.org/pmd/co/participant> ;
    owl:inverseOf <https://w3id.org/pmd/co/inputOf> .

<https://w3id.org/pmd/co/Process> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/subordinateProcess> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/Process> ],
