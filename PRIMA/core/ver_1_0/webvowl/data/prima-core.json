  }, {
    "iri" : "http://www.w3.org/2000/01/rdf-schema#Literal",
    "id" : "2",
    "label" : {
      "IRI-based" : "Literal",
      "undefined" : "Literal"
    }
  }, {
    "iri" : "https://purls.helmholtz-metadaten.de/prima/core#ResearchUser",
    "baseIri" : "https://purls.helmholtz-metadaten.de/prima/core",
    "instances" : 0,
    "annotations" : {
      "MDMCNEPGlossaryDefinition" : [ {
        "identifier" : "MDMCNEPGlossaryDefinition",
        "language" : "en",
        "value" : "Person, usually member of a project, who conducts any part of the experiment(s), or performs any of the steps of the data analysis lifecycle during the course of one or more studies, in order to collect and/or analyse research data, or is interested in reusing research data collected by a third party (e.g., reference data) with the final aim to extract insights that support the answer to some specific research question (i.e., conclusions). Research users may be assigned with a role (e.g., principal investigator, Instrument scientist, project member).",
        "type" : "label"
      } ]
    },
    "label" : {
      "IRI-based" : "ResearchUser"
    },
    "comment" : {
      "en" : "Research user is an agent or person, usually member of a project, who conducts any part of the experiment(s), or performs any of the steps of the data analysis lifecycle during the course of one or more studies."
    },
    "id" : "12",
    "superClasses" : [ "25" ]
  }, {
