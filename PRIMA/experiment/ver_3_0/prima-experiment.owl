     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:oboInOwl="http://www.geneontology.org/formats/oboInOwl#">
    <owl:Ontology rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment">
        <owl:imports rdf:resource="https://purls.helmholtz-metadaten.de/prima/core?version=3.0"/>
        <owl:imports rdf:resource="https://w3id.org/pmd/co/"/>
        <dcterms:contributor xml:lang="en">Elda Osmenaj</dcterms:contributor>
        <dcterms:contributor xml:lang="en">Mirco Panighel</dcterms:contributor>
        <dcterms:created xml:lang="en">2025-01-27</dcterms:created>
        <dcterms:creator xml:lang="en">Ahmad Zainul Ihsan</dcterms:creator>
        <dcterms:creator xml:lang="en">Mehrdad Jalali</dcterms:creator>
        <dcterms:creator xml:lang="en">Rossella Aversa</dcterms:creator>
        <dcterms:license rdf:resource="http://creativecommons.org/licenses/by/3.0/"/>
        <dcterms:title xml:lang="en">PRIMA Ontology - Experiment Module</dcterms:title>
        <vann:preferredNamespacePrefix xml:lang="en">PRIMA</vann:preferredNamespacePrefix>
        <rdfs:comment xml:lang="en">This is the experiment module of the Provenance Information for Materials Science (PRIMA) Ontology, version 3.0, aligned with PMDco v3 and based on BFO (Basic Formal Ontology).</rdfs:comment>
        <rdfs:seeAlso rdf:resource="https://github.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology"/>
        <owl:priorVersion rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment?version=2.0"/>
        <owl:versionInfo xml:lang="en">3.0</owl:versionInfo>
    </owl:Ontology>
    


