    </owl:ObjectProperty>
    


    <!-- http://www.w3.org/ns/prov#atLocation -->

    <owl:ObjectProperty rdf:about="http://www.w3.org/ns/prov#atLocation">
        <rdfs:label xml:lang="en">at location</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#carries -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#carries">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleCarrier"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Sample"/>
        <rdfs:comment xml:lang="en">carries represents the relationship between sample carrier to sample.</rdfs:comment>
        <rdfs:label xml:lang="en">carries</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasAcquisitionSoftware -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasAcquisitionSoftware">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Instrument"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#AcquisitionSoftware"/>
        <rdfs:comment xml:lang="en">hasAcquisitionSoftware represents the relationship between instrument to acquisition software</rdfs:comment>
        <rdfs:label xml:lang="en">has acquisition software</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasMaterial -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasMaterial">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isMaterialOf"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Sample"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Material"/>
        <rdfs:comment xml:lang="en">Direct relationship between Sample and Material(s) it is composed of</rdfs:comment>
        <rdfs:label xml:lang="en">has material</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurement -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurement">
        <rdfs:subPropertyOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#hasDataAcquisition"/>
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement"/>
        <rdfs:comment xml:lang="en">hasMeasurement represents the relationship between experiment to measurement.</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurementTechnique -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurementTechnique">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement"/>
        <rdfs:comment xml:lang="en">hasMeasurementTechnique represents the relationship between measurement to measurement technique.</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement technique</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasProcessingAndTreatment -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasProcessingAndTreatment">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Study"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#ProcessingAndTreatment"/>
        <rdfs:comment xml:lang="en">hasProcessingAndTreatment represents the relationship between study to processing and treatment activity.</rdfs:comment>
        <rdfs:label xml:lang="en">has processing and treatment</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#holds -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#holds">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleCarrier"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleHolder"/>
        <rdfs:comment xml:lang="en">holds represents the relationship between sample carrier to sample holder.</rdfs:comment>
        <rdfs:label xml:lang="en">holds</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hosts -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hosts">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Institution"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory"/>
        <rdfs:comment xml:lang="en">hosts represents the relationship between institution to laboratory.</rdfs:comment>
        <rdfs:label xml:lang="en">hosts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy">
        <rdfs:comment xml:lang="en">isCarriedBy is the inverse property of carries.</rdfs:comment>
        <rdfs:label xml:lang="en">is carried by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy">
        <rdfs:comment xml:lang="en">isHeldBy is the inverse property of holds.</rdfs:comment>
        <rdfs:label xml:lang="en">is held by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isMaterialOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isMaterialOf">
        <rdfs:comment xml:lang="en">isMaterialOf is the inverse property of hasMaterial.</rdfs:comment>
        <rdfs:label xml:lang="en">is material of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf">
        <rdfs:comment xml:lang="en">isMeasurementOf is the inverse property of hasMeasurement.</rdfs:comment>
        <rdfs:label xml:lang="en">is measurement of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#usesConsumable -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#usesConsumable">
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Consumable"/>
        <rdfs:comment xml:lang="en">uses is an object property relating the thing with a consumable.</rdfs:comment>
        <rdfs:label xml:lang="en">uses consumable</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- https://purls.helmholtz-metadaten.de/prima/core#Equipment -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/core#Equipment">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#atLocation"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/core#Instrument -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/core#Instrument">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#hasAcquisitionSoftware"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#AcquisitionSoftware"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/core#Study -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/core#Study">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurement"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#hasProcessingAndTreatment"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#ProcessingAndTreatment"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/dataset#RawData -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/dataset#RawData">
        <rdfs:subClassOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#ResearchData"/>
        <rdfs:comment xml:lang="en">Specific type of Research Data, primary output of a Data Acquisition performed on a System, before any subsequent Data Processing.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata"/>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#AcquisitionSoftware -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#AcquisitionSoftware">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/IAO_0000030"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#hasSetting"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Settings"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Any software designed to interface directly with an Instrument in order to control and monitor Data Acquisition parameters (i.e., Settings), and to collect Raw Data. Regardless of whether the Acquisition Software is integrated with the Instrument or operates as a standalone application (e.g., installed on a separate computer or packaged as a virtual machine), it is considered part of the Instrument.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/acquisitionsoftware"/>
        <rdfs:label xml:lang="en">Acquisition Software</rdfs:label>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/acquisitionsoftware"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#Consumable -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Consumable">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000040"/>
        <rdfs:comment xml:lang="en">Auxiliary entity, normally acquired from third-party manufacturers, used during Processing and Treatment or Measurement which has a limited time capacity or is limited in its number of uses before it is disposed of. Consumables are not part of the Instrument. Examples of Consumable are: gloves, syringes, wipes, etching solutions, glass slides, spatulas, weighing paper, two-sided tape.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/consumable"/>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/consumable"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#Institution -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Institution">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/OBI_0000245"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#hosts"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:comment xml:lang="en">Hierarchical entity which hosts one or more Laboratories.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/institution"/>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/institution"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#InstrumentScientist -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#InstrumentScientist">
        <rdfs:subClassOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#AgentRole"/>
        <rdfs:comment xml:lang="en">Person who manages a particular Instrument, or a set of them.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/instrumentscientist"/>
        <rdfs:label xml:lang="en">Instrument Scientist</rdfs:label>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/instrumentscientist"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory">
        <rdfs:subClassOf rdf:resource="http://purl.obolibrary.org/obo/BFO_0000029"/>
        <rdfs:comment xml:lang="en">Physical or virtual place hosted by an Institution, where the Equipment (including Instruments), is located and can be physically, virtually, and/or remotely accessed to perform any of the processes during the course of a Study.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/laboratory"/>
        <skos:exactMatch rdf:resource="https://purls.helmholtz-metadaten.de/skosmos/prima/laboratory"/>
    </owl:Class>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#Measurement -->

    <owl:Class rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement">
        <rdfs:subClassOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#DataAcquisition"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002211"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#ResearchUser"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002233"/>
                <owl:someValuesFrom>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/prima/core#Material"/>
                            <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Sample"/>
                        </owl:unionOf>
                    </owl:Class>
                </owl:someValuesFrom>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.obolibrary.org/obo/RO_0002234"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/dataset#RawData"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/ns/prov#atLocation"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#usesConsumable"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Consumable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
