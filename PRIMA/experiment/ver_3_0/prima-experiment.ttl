@prefix : <https://purls.helmholtz-metadaten.de/prima/experiment#> .
@prefix bfo: <http://purl.obolibrary.org/obo/BFO_> .
@prefix core: <https://purls.helmholtz-metadaten.de/prima/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix iao: <http://purl.obolibrary.org/obo/IAO_> .
@prefix obi: <http://purl.obolibrary.org/obo/OBI_> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix pmd: <https://w3id.org/pmd/co/> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix ro: <http://purl.obolibrary.org/obo/RO_> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ro:0000053 a owl:ObjectProperty ;
    rdfs:label "has participant"@en .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:creator a owl:AnnotationProperty .

dcterms:description a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

skos:exactMatch a owl:AnnotationProperty .

core:MDMCNEPGlossaryDefinition a owl:AnnotationProperty ;
    rdfs:subPropertyOf skos:definition .

<https://purls.helmholtz-metadaten.de/prima/experiment> a owl:Ontology ;
    dcterms:contributor "Elda Osmenaj"@en,
        "Mirco Panighel"@en ;
    dcterms:created "2025-01-27"@en ;
    dcterms:creator "Ahmad Zainul Ihsan"@en,
        "Mehrdad Jalali"@en,
        "Rossella Aversa"@en ;
    dcterms:license <http://creativecommons.org/licenses/by/3.0/> ;
    dcterms:title "PRIMA Ontology - Experiment Module"@en ;
    vann:preferredNamespacePrefix "PRIMA"@en ;
    rdfs:comment "This is the experiment module of the Provenance Information for Materials Science (PRIMA) Ontology, version 3.0, aligned with PMDco v3 and based on BFO (Basic Formal Ontology)."@en ;
    rdfs:seeAlso <https://github.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology> ;
    owl:imports <https://purls.helmholtz-metadaten.de/prima/core?version=3.0>,
        pmd: ;
    owl:priorVersion <https://purls.helmholtz-metadaten.de/prima/experiment?version=2.0> ;
    owl:versionInfo "3.0"@en .

:InstrumentScientist a owl:Class ;
    rdfs:label "Instrument Scientist"@en ;
    rdfs:comment "Person who manages a particular Instrument, or a set of them."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/instrumentscientist> ;
    rdfs:subClassOf core:AgentRole ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/instrumentscientist> .

:hasMeasurementTechnique a owl:ObjectProperty ;
    rdfs:label "has measurement technique"@en ;
    rdfs:comment "hasMeasurementTechnique represents the relationship between measurement to measurement technique."@en ;
    rdfs:domain :Measurement .

bfo:0000062 a owl:ObjectProperty ;
    rdfs:label "preceded by"@en ;
    owl:inverseOf bfo:0000063 .

bfo:0000063 a owl:ObjectProperty ;
    rdfs:label "precedes"@en .

skos:definition a owl:AnnotationProperty .

core:Study a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasProcessingAndTreatment ;
            owl:someValuesFrom :ProcessingAndTreatment ],
        [ a owl:Restriction ;
            owl:onProperty :hasMeasurement ;
            owl:someValuesFrom :Measurement ] .

<https://purls.helmholtz-metadaten.de/prima/dataset#RawData> a owl:Class ;
    rdfs:comment "Specific type of Research Data, primary output of a Data Acquisition performed on a System, before any subsequent Data Processing."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata> ;
    rdfs:subClassOf core:ResearchData ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/rawdata> .

:Institution a owl:Class ;
    rdfs:comment "Hierarchical entity which hosts one or more Laboratories."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/institution> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hosts ;
            owl:someValuesFrom :Laboratory ],
        obi:0000245 ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/institution> .

:carries a owl:ObjectProperty ;
    rdfs:label "carries"@en ;
    rdfs:comment "carries represents the relationship between sample carrier to sample."@en ;
    rdfs:domain :SampleCarrier ;
    rdfs:range :Sample ;
    owl:inverseOf :isCarriedBy .

:hasAcquisitionSoftware a owl:ObjectProperty ;
    rdfs:label "has acquisition software"@en ;
    rdfs:comment "hasAcquisitionSoftware represents the relationship between instrument to acquisition software"@en ;
    rdfs:domain core:Instrument ;
    rdfs:range :AcquisitionSoftware .

:hasMaterial a owl:ObjectProperty ;
    rdfs:label "has material"@en ;
    rdfs:comment "Direct relationship between Sample and Material(s) it is composed of"@en ;
    rdfs:domain :Sample ;
    rdfs:range core:Material ;
    owl:inverseOf :isMaterialOf .

:hasMeasurement a owl:ObjectProperty ;
    rdfs:label "has measurement"@en ;
    rdfs:comment "hasMeasurement represents the relationship between experiment to measurement."@en ;
    rdfs:range :Measurement ;
    rdfs:subPropertyOf core:hasDataAcquisition ;
    owl:inverseOf :isMeasurementOf .

:hasProcessingAndTreatment a owl:ObjectProperty ;
    rdfs:label "has processing and treatment"@en ;
    rdfs:comment "hasProcessingAndTreatment represents the relationship between study to processing and treatment activity."@en ;
    rdfs:domain core:Study ;
    rdfs:range :ProcessingAndTreatment .

:holds a owl:ObjectProperty ;
    rdfs:label "holds"@en ;
    rdfs:comment "holds represents the relationship between sample carrier to sample holder."@en ;
    rdfs:domain :SampleCarrier ;
    rdfs:range :SampleHolder ;
    owl:inverseOf :isHeldBy .

:hosts a owl:ObjectProperty ;
    rdfs:label "hosts"@en ;
    rdfs:comment "hosts represents the relationship between institution to laboratory."@en ;
    rdfs:domain :Institution ;
    rdfs:range :Laboratory .

:isMaterialOf a owl:ObjectProperty ;
    rdfs:label "is material of"@en ;
    rdfs:comment "isMaterialOf is the inverse property of hasMaterial."@en .

:isMeasurementOf a owl:ObjectProperty ;
    rdfs:label "is measurement of"@en ;
    rdfs:comment "isMeasurementOf is the inverse property of hasMeasurement."@en .

ro:0002211 a owl:ObjectProperty ;
    rdfs:label "has agent"@en .

ro:0002233 a owl:ObjectProperty ;
    rdfs:label "has input"@en .

ro:0002234 a owl:ObjectProperty ;
    rdfs:label "has output"@en .

:AcquisitionSoftware a owl:Class ;
    rdfs:label "Acquisition Software"@en ;
    rdfs:comment "Any software designed to interface directly with an Instrument in order to control and monitor Data Acquisition parameters (i.e., Settings), and to collect Raw Data. Regardless of whether the Acquisition Software is integrated with the Instrument or operates as a standalone application (e.g., installed on a separate computer or packaged as a virtual machine), it is considered part of the Instrument."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/acquisitionsoftware> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty core:hasSetting ;
            owl:someValuesFrom core:Settings ],
        iao:0000030 ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/acquisitionsoftware> .

:SampleHolder a owl:Class ;
    rdfs:comment "Piece of Equipment used for making one or more Samples accessible for a Measurement or for holding them in place in the predefined position to be mounted inside the Instrument. Examples of Sample Holder are: a glass slide, a TEM grid, or a tilting support. Depending on the scientific use case, a Sample Holder may carry or be carried by a Sample Carrier."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/sampleholder> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :isHeldBy ;
            owl:someValuesFrom :SampleCarrier ],
        core:Equipment ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/sampleholder> .

:isCarriedBy a owl:ObjectProperty ;
    rdfs:label "is carried by"@en ;
    rdfs:comment "isCarriedBy is the inverse property of carries."@en .

:isHeldBy a owl:ObjectProperty ;
    rdfs:label "is held by"@en ;
    rdfs:comment "isHeldBy is the inverse property of holds."@en .

:usesConsumable a owl:ObjectProperty ;
    rdfs:label "uses consumable"@en ;
    rdfs:comment "uses is an object property relating the thing with a consumable."@en ;
    rdfs:range :Consumable .

prov:atLocation a owl:ObjectProperty ;
    rdfs:label "at location"@en .

core:Equipment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom :Laboratory ] .

core:Instrument a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasAcquisitionSoftware ;
            owl:someValuesFrom :AcquisitionSoftware ] .

:Consumable a owl:Class ;
    rdfs:comment "Auxiliary entity, normally acquired from third-party manufacturers, used during Processing and Treatment or Measurement which has a limited time capacity or is limited in its number of uses before it is disposed of. Consumables are not part of the Instrument. Examples of Consumable are: gloves, syringes, wipes, etching solutions, glass slides, spatulas, weighing paper, two-sided tape."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/consumable> ;
    rdfs:subClassOf bfo:0000040 ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/consumable> .

:Measurement a owl:Class ;
    rdfs:comment "Experimental Data Acquisition performed by one or more Research Users on one or more physical Systems (Materials or Samples) using an experimental Technique. One or more Measurements may also be performed during Processing and Treatment, e.g., to characterize the intermediate stages and/or the final resulting physical System(s). A Measurement may require the use of Consumables."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/measurement> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onClass core:Instrument ;
            owl:onProperty core:usesEquipment ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002211 ;
            owl:someValuesFrom core:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002234 ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002233 ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( core:Material :Sample ) ] ],
        [ a owl:Restriction ;
            owl:onProperty :usesConsumable ;
            owl:someValuesFrom :Consumable ],
        [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom :Laboratory ],
        core:DataAcquisition ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/measurement> .

:ProcessingAndTreatment a owl:Class ;
    rdfs:comment "Set of actions (physical changes or chemical reactions) carried out by one or more Research Users, a commercial enterprise, or a third party, performed on (or between) one or more physical Systems to produce them, treat them, or prepare them for a subsequent Measurement, under controlled conditions described by Settings. Processing and Treatment may require the use of Equipment and Consumables. One or more Measurements may also be performed during Processing and Treatment, e.g., to characterize the intermediate stages and/or the final resulting physical System(s). The output of Processing and Treatment is one or more physical Systems. Examples of Processing and Treatment are: synthesis, fabrication, any type of Material or Sample treatment (e.g., thermo-mechanical or surface treatment), and Sample preparation."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/processingandtreatment> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty bfo:0000062 ;
            owl:someValuesFrom :ProcessingAndTreatment ],
        [ a owl:Restriction ;
            owl:onProperty core:hasDataAcquisition ;
            owl:someValuesFrom core:DataAcquisition ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002234 ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( core:Material :Sample ) ] ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002233 ;
            owl:someValuesFrom [ a owl:Class ;
                    owl:unionOf ( core:Material :Sample ) ] ],
        [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom :Laboratory ],
        [ a owl:Restriction ;
            owl:onProperty core:usesEquipment ;
            owl:someValuesFrom core:Equipment ],
        [ a owl:Restriction ;
            owl:onProperty :usesConsumable ;
            owl:someValuesFrom :Consumable ],
        [ a owl:Restriction ;
            owl:onProperty ro:0002211 ;
            owl:someValuesFrom core:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty core:usesEquipment ;
            owl:someValuesFrom core:Instrument ],
        [ a owl:Restriction ;
            owl:onProperty core:hasTechnique ;
            owl:someValuesFrom core:Technique ],
        obi:0000011 ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/processingandtreatment> .

:SampleCarrier a owl:Class ;
    rdfs:comment "Piece of Equipment used for carrying one or more Samples and/or one or more Sample Holders (depending on the scientific use case), which is helpful for, e.g., referencing, handling, or height adjustment. Examples of Sample Carrier are: a naked wafer, a glass slide, an individually designed metal frame, but also a Si crystal carrying a Sample of exfoliated graphene."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/samplecarrier> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :carries ;
            owl:someValuesFrom :Sample ],
        [ a owl:Restriction ;
            owl:onProperty :holds ;
            owl:someValuesFrom :SampleHolder ],
        core:Equipment ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/samplecarrier> .

:Laboratory a owl:Class ;
    rdfs:comment "Physical or virtual place hosted by an Institution, where the Equipment (including Instruments), is located and can be physically, virtually, and/or remotely accessed to perform any of the processes during the course of a Study."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/laboratory> ;
    rdfs:subClassOf bfo:0000029 ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/laboratory> .

:Sample a owl:Class ;
    rdfs:comment "Physical System, composed by one or more Materials, exposed to the Instrument during a Measurement, typically after some kind of Processing and Treatment. Sample may be held by a Sample Holder and/or carried by a Sample Carrier."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/sample> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty :hasMaterial ;
            owl:someValuesFrom core:Material ],
        [ a owl:Restriction ;
            owl:onProperty :isCarriedBy ;
            owl:someValuesFrom :SampleCarrier ],
        core:System ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/sample> .

