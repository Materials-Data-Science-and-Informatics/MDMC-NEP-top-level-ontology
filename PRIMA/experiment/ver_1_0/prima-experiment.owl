        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Sample"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleComponent"/>
        <rdfs:comment xml:lang="en">hasSampleComponent represents the relationship between sample to sample component</rdfs:comment>
        <rdfs:label xml:lang="en">has sample component</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasSamplePreparation -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasSamplePreparation">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Experiment"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SamplePreparation"/>
        <rdfs:comment xml:lang="en">hasSamplePreparation represents the relationship between experiment to sample preparation activity.</rdfs:comment>
        <rdfs:label xml:lang="en">has sample preparation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#holds -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#holds">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleCarrier"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleHolder"/>
        <rdfs:comment xml:lang="en">holds represents the relationship between sample carrier to sample holder.</rdfs:comment>
        <rdfs:label xml:lang="en">holds</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hosts -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hosts">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Institution"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory"/>
        <rdfs:comment xml:lang="en">hosts represents the relationship between institution to laboratory.</rdfs:comment>
        <rdfs:label xml:lang="en">hosts</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy">
        <rdfs:comment xml:lang="en">isCarriedBy is the inverse property of carries.</rdfs:comment>
        <rdfs:label xml:lang="en">is carried by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isHeldBy">
        <rdfs:comment xml:lang="en">isHeldBy is the inverse property of holds.</rdfs:comment>
        <rdfs:label xml:lang="en">is held by</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf">
        <rdfs:comment xml:lang="en">isMeasurementOf is the inverse property of hasMeasurement.</rdfs:comment>
        <rdfs:label xml:lang="en">is measurement of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#isSampleComponentOf -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#isSampleComponentOf">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment xml:lang="en">isSampleComponentOf is the inverse property of hasSampleComponent.</rdfs:comment>
        <rdfs:label xml:lang="en">is sample component of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#wasOperatedBy -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#wasOperatedBy">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/ns/prov#wasInfluencedBy"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Instrument"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#ResearchUser"/>
        <rdfs:comment xml:lang="en">wasOperatedBy represents the provenance information that connects the agent that was operated by another agent, e.g., instrument was operated by research user.</rdfs:comment>
        <rdfs:label xml:lang="en">was operated by</rdfs:label>
    </owl:ObjectProperty>
    


