    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#carries -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#carries">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isCarriedBy"/>
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#SampleCarrier"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Sample"/>
        <rdfs:comment xml:lang="en">carries represents the relationship between sample carrier to sample.</rdfs:comment>
        <rdfs:label xml:lang="en">carries</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasFabrication -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasFabrication">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/core#Experiment"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Fabrication"/>
        <rdfs:comment xml:lang="en">hasFabrication represents the relationship between experiment to fabrication activity.</rdfs:comment>
        <rdfs:label xml:lang="en">has fabrication</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurement -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurement">
        <owl:inverseOf rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#isMeasurementOf"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/prima/core#Experiment"/>
                    <rdf:Description rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#Fabrication"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement"/>
        <rdfs:comment xml:lang="en">hasMeasurement represents the relationship between experiment to measurement.</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurementTechnique -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasMeasurementTechnique">
        <rdfs:domain rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Measurement"/>
        <rdfs:range rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#MeasurementTechnique"/>
        <rdfs:comment xml:lang="en">hasMeasurementTechnique represents the relationship between measurement to measurement technique.</rdfs:comment>
        <rdfs:label xml:lang="en">has measurement technique</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- https://purls.helmholtz-metadaten.de/prima/experiment#hasSampleComponent -->

    <owl:ObjectProperty rdf:about="https://purls.helmholtz-metadaten.de/prima/experiment#hasSampleComponent">
