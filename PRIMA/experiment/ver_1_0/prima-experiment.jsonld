    "http://www.w3.org/2000/01/rdf-schema#comment": [
      {
        "@language": "en",
        "@value": "Laboratory is place (could also be virtual) hosted by an institution, where one or more Instruments, as well as the equipment, are located and the measurement is performed."
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#subClassOf": [
      {
