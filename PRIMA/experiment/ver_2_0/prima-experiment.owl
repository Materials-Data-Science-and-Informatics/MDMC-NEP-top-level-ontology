        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#uses"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Consumable"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="https://w3id.org/pmd/co/input"/>
                <owl:someValuesFrom rdf:resource="https://purls.helmholtz-metadaten.de/prima/experiment#Precursor"/>
