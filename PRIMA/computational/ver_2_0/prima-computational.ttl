@prefix : <https://purls.helmholtz-metadaten.de/prima/computational#> .
@prefix core: <https://purls.helmholtz-metadaten.de/prima/core#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<http://nfdi.fiz-karlsruhe.de/ontology/Specification> a owl:Class ;
    rdfs:subClassOf prov:Entity .

dcterms:contributor a owl:AnnotationProperty .

dcterms:created a owl:AnnotationProperty .

dcterms:license a owl:AnnotationProperty .

dcterms:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

<http://qudt.org/schema/qudt/Quantity> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/QuantityValue> ;
            owl:onProperty <http://qudt.org/schema/qudt/quantityValue> ],
        [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/QuantityKind> ;
            owl:onProperty <http://qudt.org/schema/qudt/hasQuantityKind> ] .

<http://w3id.org/STREAM/MSLE#Equipment> a owl:Class ;
    rdfs:label "equipment"@en ;
    owl:equivalentClass core:Equipment .

skos:exactMatch a owl:AnnotationProperty .

prov:SoftwareAgent a owl:Class ;
    rdfs:subClassOf prov:Agent .

<https://purls.helmholtz-metadaten.de/prima/computational> a owl:Ontology ;
    dcterms:contributor "Ahmad Zainul Ihsan",
        "Rossella Aversa" ;
    dcterms:created "2024-05-15" ;
    dcterms:license <http://creativecommons.org/licenses/by/3.0/> ;
    dcterms:title "PRIMA Ontology - Computational Workflow Module" ;
    vann:preferredNamespacePrefix "PRIMA" ;
    rdfs:comment "This is the computational workflow module of the Provenance Information for Materials Science (PRIMA) Ontology" ;
    rdfs:seeAlso <https://github.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology> ;
    owl:imports <https://purls.helmholtz-metadaten.de/prima/core> ;
    owl:versionInfo 2.0 .

:Calculation a owl:Class ;
    rdfs:label "Calculation"@en ;
    rdfs:comment "Computational Data Acquisition performed by one or more Research Users on a Model to process its input Settings into output calculated properties using a specific computational and/or theoretical Technique based on a theory accepted by the scientific community (e.g., Density Functional Theory, Conformal Field Theory)."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/calculation> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty core:hasTechnique ;
            owl:someValuesFrom core:Technique ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom :Model ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        core:DataAcquisition ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/calculation> .

:ModelPreparation a owl:Class ;
    rdfs:label "Model Preparation"@en ;
    rdfs:comment "Set of actions carried out by one or more Research Users, possibly using Research Software, to define and/or formulate a Model making use of Research Data (typically, but not limited to, Reference Data after Data Processing) as input for parametrization, boundary conditions, or validation. A Model Preparation may require the use of Equipment. The output of Model Preparation is Model."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/modelpreparation> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom core:ResearchData ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/experiment#ResearchUser> ],
        [ a owl:Restriction ;
            owl:onProperty core:usesResearchSoftware ;
            owl:someValuesFrom core:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty core:usesEquipment ;
            owl:someValuesFrom core:Equipment ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty core:hasTechnique ;
            owl:someValuesFrom core:Technique ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom :Model ],
        <https://w3id.org/pmd/co/Process> ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/modelpreparation> .

:Simulation a owl:Class ;
    rdfs:label "Simulation"@en ;
    rdfs:comment "Computational Data Acquisition performed by one or more Research Users on a Model to manipulate its Settings using a specific computational and/or theoretical Technique in order to study, predict, or optimize the behaviour and performance of existing or proposed features and properties of a physical System that would otherwise be too complex, too large/small, too fast/slow, too dangerous, inaccessible, or unacceptable to engage and/or control. Examples of Simulation are: multiscale simulation, finite-element simulation, molecular dynamics simulation, discrete dislocation dynamics simulation."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/simulation> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom :Model ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty core:hasTechnique ;
            owl:someValuesFrom core:Technique ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        core:DataAcquisition ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/simulation> .

core:MDMCNEPGlossaryDefinition a owl:AnnotationProperty ;
    rdfs:subPropertyOf skos:definition .

core:hasDataAcquisition a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dal#DataProcessing> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/experiment#ResearchUser> ],
        [ a owl:Restriction ;
            owl:onProperty core:usesEquipment ;
            owl:someValuesFrom core:Equipment ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#RawData> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> ],
        [ a owl:Restriction ;
            owl:onProperty core:usesResearchSoftware ;
            owl:someValuesFrom core:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom core:ResearchData ],
        <https://w3id.org/pmd/co/Process> .

<https://purls.helmholtz-metadaten.de/prima/experiment#Institution> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://purls.helmholtz-metadaten.de/prima/experiment#hosts> ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory> ],
        prov:Organization .

<http://qudt.org/schema/qudt/QuantityKind> a owl:Class .

<http://qudt.org/schema/qudt/QuantityValue> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://qudt.org/schema/qudt/Unit> ;
            owl:onProperty <http://qudt.org/schema/qudt/unit> ] .

<http://qudt.org/schema/qudt/Unit> a owl:Class .

<http://qudt.org/schema/qudt/hasQuantityKind> a owl:ObjectProperty .

<http://qudt.org/schema/qudt/quantityValue> a owl:ObjectProperty .

<http://qudt.org/schema/qudt/unit> a owl:ObjectProperty .

skos:definition a owl:AnnotationProperty .

prov:Activity a owl:Class .

prov:Location a owl:Class .

prov:Organization a owl:Class ;
    rdfs:subClassOf prov:Agent .

prov:wasInformedBy a owl:ObjectProperty .

core:operates a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/experiment#hosts> a owl:ObjectProperty .

<https://w3id.org/pmd/co/DigitalEntity> a owl:Class ;
    rdfs:subClassOf prov:Entity .

<https://w3id.org/pmd/co/characteristicOf> a owl:ObjectProperty ;
    rdfs:domain [ a owl:Class ;
            owl:unionOf ( <https://w3id.org/pmd/co/Object> <https://w3id.org/pmd/co/Process> <https://w3id.org/pmd/co/ValueObject> ) ] ;
    rdfs:range <https://w3id.org/pmd/co/ValueObject> .

<https://w3id.org/pmd/co/inputOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/nextProcess> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range <https://w3id.org/pmd/co/Process> ;
    rdfs:subPropertyOf prov:wasInformedBy ;
    owl:inverseOf <https://w3id.org/pmd/co/previousProcess> .

<https://w3id.org/pmd/co/outputOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/participantOf> a owl:ObjectProperty .

<https://w3id.org/pmd/co/previousProcess> a owl:ObjectProperty .

<https://w3id.org/pmd/co/subordinateProcess> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range <https://w3id.org/pmd/co/Process> .

prov:atLocation a owl:ObjectProperty .

prov:wasAssociatedWith a owl:ObjectProperty .

core:DataAcquisition a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom <https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory> ] .

core:ResearchSoftware a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty core:hasSetting ;
            owl:someValuesFrom core:Settings ] .

core:hasSetting a owl:ObjectProperty .

core:usesEquipment a owl:ObjectProperty .

core:usesResearchSoftware a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/experiment#ResearchUser> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty core:operates ;
            owl:someValuesFrom core:Equipment ],
        prov:Agent .

<https://w3id.org/pmd/co/characteristic> a owl:ObjectProperty ;
    owl:inverseOf <https://w3id.org/pmd/co/characteristicOf> .

<https://w3id.org/pmd/co/participant> a owl:ObjectProperty ;
    rdfs:domain <https://w3id.org/pmd/co/Process> ;
    rdfs:range [ a owl:Class ;
            owl:unionOf ( <https://w3id.org/pmd/co/DigitalEntity> <https://w3id.org/pmd/co/Object> <https://w3id.org/pmd/co/ValueObject> ) ] ;
    owl:inverseOf <https://w3id.org/pmd/co/participantOf> .

prov:Agent a owl:Class .

:Model a owl:Class ;
    rdfs:label "Model"@en ;
    rdfs:comment "Digital representation of a System, primary output of any kind of Model Preparation, aimed to be used in one or more Calculations or Simulations for its description or for predictions of its behaviour. A Model represents the System by direct similitude (e.g., small-scale replica) or by capturing in a logical framework the relations between its properties (e.g., mathematical Model). A Model typically consists of Settings which may be stored in one or more files."@en ;
    rdfs:isDefinedBy <https://purls.helmholtz-metadaten.de/skosmos/prima/model> ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty core:hasSetting ;
            owl:someValuesFrom core:Settings ],
        core:System ;
    skos:exactMatch <https://purls.helmholtz-metadaten.de/skosmos/prima/model> .

core:hasTechnique a owl:ObjectProperty .

<https://purls.helmholtz-metadaten.de/prima/dataset#RawData> a owl:Class ;
    rdfs:subClassOf core:ResearchData .

<https://purls.helmholtz-metadaten.de/prima/experiment#Laboratory> a owl:Class ;
    rdfs:subClassOf prov:Location .

prov:Entity a owl:Class .

<https://purls.helmholtz-metadaten.de/prima/dataset#ProcessedData> a owl:Class ;
    rdfs:subClassOf core:ResearchData .

<https://w3id.org/pmd/co/Object> a owl:Class ;
    rdfs:subClassOf prov:Entity .

<https://w3id.org/pmd/co/ValueObject> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/characteristic> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/ValueObject> ],
        prov:Entity .

<https://w3id.org/pmd/co/output> a owl:ObjectProperty ;
    rdfs:subPropertyOf <https://w3id.org/pmd/co/participant> ;
    owl:inverseOf <https://w3id.org/pmd/co/outputOf> .

<https://w3id.org/pmd/co/input> a owl:ObjectProperty ;
    rdfs:subPropertyOf <https://w3id.org/pmd/co/participant> ;
    owl:inverseOf <https://w3id.org/pmd/co/inputOf> .

<https://w3id.org/pmd/co/Process> a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/output> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/Object> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/input> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/Object> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/subordinateProcess> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/Process> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/characteristic> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/ValueObject> ],
        [ a owl:Restriction ;
            owl:onProperty <https://w3id.org/pmd/co/nextProcess> ;
            owl:someValuesFrom <https://w3id.org/pmd/co/Process> ],
        prov:Activity .

