    mdmc:hasDataAnalysisLifeCycle ex:image_labelling_process_1 ;
    mdmc:hasExperiment ex:STM_experiment_1998 .

ex:study_2 a mdmc:Study ;
    mdmc:hasExperiment ex:STM_experiment_2010 .

prov:Collection a owl:Class ;
    rdfs:subClassOf prov:Entity .

prov:Location a owl:Class .

prov:Organization a owl:Class ;
    rdfs:subClassOf prov:Agent .

prov:SoftwareAgent a owl:Class ;
    rdfs:subClassOf prov:Agent .

prov:wasInfluencedBy a owl:ObjectProperty .

mdmc:AnalysedData a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:DataAnalysis ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        mdmc:ProcessedDataOrAnalysedData ;
    skos:definition "Research Data which is the primary output of any kind of Data Analysis performed on Research Data, typically on Processed Data, by one or more Research Users, possibly using Research Software. Analysed Data is typically in the form of a data file, but it may potentially be a data stream or any other form of data which is relevant in a particular data management context. Analysed Data may be stored in a Data Collaboration Platform and/or in a Data Repository. Analysed Data may be part of a Dataset."@en .

mdmc:DataAnalysis a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:isMemberOf ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:ResearchData ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        prov:Activity ;
    skos:definition "Data treatment performed by one or more Research Users, who collect, model and analyse Research Data, typically Processed Data, to extract insights that support the answer to some specific research question (i.e., Conclusions). Data Analysis may include: linear combination, data fitting, data modeling, pattern extraction. Data Analysis may be performed using Research Software. The output of Data Analysis is Analysed Data."@en .

mdmc:DataCollaborationPlatform a owl:Class ;
    skos:definition "Operational information system which allows Research Users to keep their Research Data, Datasets and related documents (e.g., drafts of Scientific Publications) synchronized and up to date, and to exchange them with other Research Users, who are typically members of the same Project. The system is intended for the long-tail and still volatile data, which can change and are still subject to active research. Therefore, a Data Collaboration Platform offers versioning of all ingested files but does not usually assign Persistent Identifiers to them."@en .

mdmc:DataInterpretation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:isMemberOf ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:ResearchData ],
        prov:Activity ;
    skos:definition "Process performed by one or more Research Users, who assign meaning to Research Data, typically Analysed Data, and determine the Conclusions of the Study, possibly in comparison with Reference Data. Data Interpretation supports decision-making about the next steps of the Study or about future work. Data Interpretation may be performed using Research Software."@en .

mdmc:Institution a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:hosts ;
            owl:someValuesFrom mdmc:Laboratory ],
        prov:Organization ;
    skos:definition "Hierarchical entity which hosts one or more Laboratories, including the virtual ones."@en .

mdmc:MeasurementTechnique a owl:Class ;
    skos:definition "Technique or technology corresponding to the method used during a Measurement to collect Raw Data about a Sample or a set of them with an Instrument."@en .

mdmc:MetadataRepository a owl:Class ;
    skos:definition "Information system used to store, manage and provide access to Metadata, following a policy or a set of rules that define storage and access norms. Metadata Repositories may be associated with an Institution or a group of them, or may be run by a third party. Metadata Repositories may or may not be directly used by Research Users."@en .

mdmc:carries a owl:ObjectProperty ;
    owl:inverseOf mdmc:isCarriedBy .

mdmc:hasDataAnalysisLifeCycle a owl:ObjectProperty .

mdmc:hasExperiment a owl:ObjectProperty .

mdmc:hasMeasurementTechnique a owl:ObjectProperty .

mdmc:hasSampleComponent a owl:ObjectProperty ;
    owl:inverseOf mdmc:isSampleComponentOf .

mdmc:hasSampleComponentFabrication a owl:ObjectProperty .

mdmc:hasSamplePreparation a owl:ObjectProperty .

mdmc:hasStudy a owl:ObjectProperty .

mdmc:holds a owl:ObjectProperty ;
    owl:inverseOf mdmc:isHeldBy .

mdmc:hosts a owl:ObjectProperty ;
    owl:inverseOf mdmc:isHostedBy .

mdmc:stores a owl:ObjectProperty .

<https://raw.githubusercontent.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology/master/mdmc-nep-top-level-ontology.owl/wasOperatedBy> a owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasInfluencedBy .

ex:Alice a mdmc:ResearchUser ;
    prov:actedOnBehalfOf ex:STRAS_Research_Group .

ex:Graphene_growth_sample_preparation a mdmc:SamplePreparation ;
    prov:used ex:ethylene_1,
        ex:sample_1_component_Ni ;
    prov:wasAssociatedWith ex:gas_line_1,
        ex:heating_gun_1,
        ex:sputter_gun_1 .

ex:Structured_and_FAIR_dataset_1 a mdmc:ProcessedDataOrAnalysedData ;
    prov:wasAttributedTo ex:Bob,
        ex:STRAS_Research_Group ;
    prov:wasGeneratedBy ex:image_selection_and_retrievement_1 .

ex:VT_STM_measurement_1998_1 a mdmc:Measurement ;
    prov:endedAtTime "1998-05-20"^^xsd:date ;
    prov:startedAtTime "1998-04-20"^^xsd:date ;
    prov:used ex:graphe_sample ;
    prov:wasAssociatedWith ex:STRAS_Research_Group,
        ex:VT_STM_Microscopy .

ex:VT_STM_measurement_1998_2 a mdmc:Measurement ;
    prov:endedAtTime "1998-05-21"^^xsd:date ;
    prov:startedAtTime "1998-04-21"^^xsd:date .

ex:graphe_sample a mdmc:Sample ;
    prov:wasGeneratedBy ex:Graphene_growth_sample_preparation .

dcterms:isReferencedBy a owl:ObjectProperty .

prov:hadMember a owl:ObjectProperty .

prov:wasDerivedFrom a owl:ObjectProperty .

mdmc:Conclusion a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:DataInterpretation ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:Conclusion ],
        prov:Entity ;
    skos:definition "The primary output of Data Interpretation performed by one or more Research Users on Research Data, typically on Analysed Data, possibly using Research Software. Conclusions are any kind of insight that support the answer to some specific research question, such as the significance and implications of the research findings of a Study, possibly in comparison with Reference Data, along with recommendations which may support decision-making about the next steps of a Study or about future work. Conclusions form an important part of a Study debrief and are usually reported in Scientific Publications."@en .

mdmc:DataRepository a owl:Class ;
    skos:definition "Information system used to store, manage and provide access to digital resources, following a set of rules that define storage and access norms. A Data Repository is particularly suitable for Research Data (especially Datasets and/or Publication Data) which are not likely to be altered again. Many Data Repositories automatically assign globally unique Persistent Identifiers to deposited resources. Data Repositories may be associated with an Institution or a group of them, with an Instrument or a group of them, or with a Measurement Technique or a group of them, or may be run by a third party. Data Repositories may or may not be directly used by Research Users."@en .

mdmc:Dataset a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:hasMetadata ;
            owl:someValuesFrom mdmc:Metadata ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isStoredIn ;
            owl:someValuesFrom mdmc:DataCollaborationPlatform ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isStoredIn ;
            owl:someValuesFrom mdmc:DataRepository ],
        [ a owl:Restriction ;
            owl:onProperty prov:hadMember ;
            owl:someValuesFrom mdmc:ResearchData ],
        [ a owl:Restriction ;
            owl:onProperty prov:hadMember ;
            owl:someValuesFrom mdmc:Dataset ],
        prov:Collection ;
    skos:definition "Collection of scientifically related (depending on the research context) Research Data, along with their respective descriptive Metadata, typically stored in a Data Collaboration Platform and/or in a Data Repository. A Dataset may consist of Raw Data (including the output of computational Experiments), Processed Data, Analysed Data, or other Datasets. The components of a Dataset remain individually identifiable."@en .

mdmc:ProcessedData a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:DataProcessing ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        mdmc:ProcessedDataOrAnalysedData ;
    skos:definition "Research Data which is the primary output of any kind of Data Processing or manipulation of Raw Data performed by one or more Research Users, possibly using Research Software, in order to prepare it for another step of the Data Analysis Lifecycle, e.g., Data Analysis or Data Interpretation. Processed Data is typically in the form of a data file, but it may potentially be a data stream or any other form of data which is relevant in a particular data management context. Processed Data may be stored in a Data Collaboration Platform and/or in a Data Repository. Processed Data may be part of a Dataset."@en .

mdmc:RawData a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:Measurement ],
        mdmc:ResearchData ;
    skos:definition "Research Data which is the primary output of a Measurement, collected by one or more Research Users using an Instrument, before any subsequent Data Processing. Raw Data may be the output of a simulation run (computational Measurement). Raw Data is typically in the form of a data file, but it may potentially be a data stream or any other form of data which is relevant in a particular data management context. Raw Data may be stored in a Data Collaboration Platform and/or in a Data Repository. Raw Data may be part of a Dataset."@en .

mdmc:SampleCarrier a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:holds ;
            owl:someValuesFrom mdmc:SampleHolder ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:carries ;
            owl:someValuesFrom mdmc:Sample ],
        mdmc:Equipment ;
    skos:definition "A piece of Equipment used for carrying one or more Samples and/or one or more Sample Holders, helpful, e.g., for referencing, handling or height adjustment. Sample Carrier may be, e.g., a naked wafer, a glass slide or an individually designed metal frame."@en .

mdmc:SampleComponentFabrication a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:SampleComponent ],
        [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom mdmc:Laboratory ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:Instrument ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:Equipment ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasMeasurement ;
            owl:someValuesFrom mdmc:Measurement ],
        prov:Activity ;
    skos:definition "Fabrication of a Sample Component in controlled conditions, performed by a commercial enterprise, a Research User or a third party. Sample Component Fabrication may require the use of Equipment and Instrument(s). A Measurement may also be performed during the Sample Component Synthesis, e.g., to characterize the resulting Sample Component."@en .

mdmc:SampleHolder a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:isHeldBy ;
            owl:someValuesFrom mdmc:SampleCarrier ],
        mdmc:Equipment ;
    skos:definition "A piece of Equipment that makes one or more Samples accessible for a Measurement, or holds them in place in the predefined position to be mounted inside the Instrument (e.g., glass slide, TEM grid, tilting support). Sample Holder(s) may be carried by a Sample Carrier."@en .

mdmc:isCarriedBy a owl:ObjectProperty .

mdmc:isHeldBy a owl:ObjectProperty .

mdmc:isHostedBy a owl:ObjectProperty .

mdmc:isMeasurementOf a owl:ObjectProperty .

mdmc:isSampleComponentOf a owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty .

prov:wasAttributedTo a owl:ObjectProperty .

mdmc:Metadata a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:isStoredIn ;
            owl:someValuesFrom mdmc:MetadataRepository ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAttributedTo ;
            owl:someValuesFrom mdmc:ResearchUser ],
        prov:Entity ;
    skos:definition "Any descriptive data intended to help contextualize or otherwise qualify Research Data and/or Datasets and/or Publication Data and their management through time. Depending on the mode of use, Metadata describes information pertaining to the research Projects, including (but not limited to) Research Users, Studies, Experiments, Measurements, Instruments, Samples, and corresponding Data Analysis Lifecycle. Metadata may include descriptions of how files are named, structured and stored. Metadata may be registered in a Metadata Repository."@en .

mdmc:SamplePreparation a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:Instrument ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasMeasurement ;
            owl:someValuesFrom mdmc:Measurement ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:Equipment ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:Sample ],
        [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom mdmc:Laboratory ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:SampleComponent ],
        prov:Activity ;
    skos:definition "Identifiable and reproducible set of actions (physical changes or chemical reactions) typically carried out by one or more Research Users to produce one or more Samples and/or to make the Sample(s) fit to perform a Measurement. The actions may be performed on (or between) one or more Sample Components, or on a Sample. Sample Preparation may require the use of Equipment and Instrument(s). A Measurement may also be performed during the Sample Preparation, e.g., to characterize the resulting Sample or Sample Component(s)."@en .

mdmc:Study a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasExperiment ;
            owl:someValuesFrom mdmc:Experiment ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasDataAnalysisLifeCycle ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        prov:Entity ;
    skos:definition "Set of one or more Experiments and corresponding Data Analysis Lifecycle performed by one or more Research Users who are part of the same Project."@en .

mdmc:hasMeasurement a owl:ObjectProperty ;
    owl:inverseOf mdmc:isMeasurementOf .

mdmc:hasMetadata a owl:ObjectProperty .

mdmc:isMemberOf a owl:ObjectProperty .

ex:Bob a mdmc:ResearchUser .

ex:image_labelling_process_1 a mdmc:DataAnalysisLifeCycle ;
    prov:used ex:Reference_Dataset_1 ;
    prov:wasAssociatedWith ex:Bob,
        ex:STRAS_Research_Group .

prov:Agent a owl:Class .

prov:atLocation a owl:ObjectProperty .

mdmc:DataProcessing a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:ResearchData ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isMemberOf ;
            owl:someValuesFrom mdmc:DataAnalysisLifeCycle ],
        prov:Activity ;
    skos:definition "Data treatment performed by one or more Research Users on Research Data, typically Raw Data, to prepare it for another step of the Data Analysis Lifecycle, e.g., Data Analysis or Data Interpretation. Data Processing usually consists of routine actions. It may include: filtering, denoising, transformation, fusion or compression of existing Research Data, as well as calibration, normalization, statistical data reduction, background subtraction, correction of artifacts. Data Processing may be performed using Research Software. The output of Data Processing is Processed Data."@en .

mdmc:Experiment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:hasSamplePreparation ;
            owl:someValuesFrom mdmc:SamplePreparation ],
        [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom mdmc:Laboratory ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasMeasurement ;
            owl:someValuesFrom mdmc:Measurement ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasSampleComponentFabrication ;
            owl:someValuesFrom mdmc:SampleComponentFabrication ],
        prov:Activity ;
    skos:definition "Identifiable and reproducible activity with a clear start time and end time, which may include a set of one or more Sample Component Syntheses, Sample Preparations and/or Measurements, performed by one or more Research Users. An Experiment may be a simulation (computational Experiment) or a combination of computational and physical Measurement."@en .

mdmc:ProcessedDataOrAnalysedData a owl:Class ;
    rdfs:subClassOf mdmc:ResearchData ;
    owl:disjointUnionOf ( mdmc:AnalysedData mdmc:ProcessedData ) .

mdmc:isStoredIn a owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    owl:inverseOf mdmc:stores .

mdmc:Instrument a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty <https://raw.githubusercontent.com/Materials-Data-Science-and-Informatics/MDMC-NEP-top-level-ontology/master/mdmc-nep-top-level-ontology.owl/wasOperatedBy> ;
            owl:someValuesFrom mdmc:ResearchUser ],
        mdmc:Equipment ;
    skos:definition "Identifiable piece of Equipment used by one or more Research Users to perform a Measurement and to generate Raw Data. Instrument is located in a Laboratory hosted by an Institution. Instrument may also stand for a software, a software module and/or a particular configuration of it, used to perform a simulation run (computational Measurement)."@en .

mdmc:Laboratory a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:isHostedBy ;
            owl:someValuesFrom mdmc:Institution ],
        prov:Location ;
    skos:definition "Place (could also be virtual) hosted by an Institution, where one or more Instruments, as well as the Equipment, are located and the Measurement is performed. Laboratory may be the hardware and/or the software platform and/or the services which allow to order and manage computational Experiments. In this case, the software platform (virtual Laboratory) serves the purpose of managing software modules (virtual Instruments)."@en .

mdmc:ResearchSoftware a owl:Class ;
    rdfs:subClassOf prov:SoftwareAgent ;
    skos:definition "Software used to generate, process, analyse or access Research Data during any of the processes included in the Data Analysis Lifecycle (possibly including data rendering, visualization, plotting). Depending on the research context, Research Software can be used during Data Processing, Data Analysis or Data Interpretation taking as input Raw Data, Processed Data or Analysed Data, respectively, and giving as output Processed Data, Analysed Data or any kind of synthesized knowledge (e.g., Conclusions), respectively. Any acquisition software used during Measurements is considered part of the Instrument and should be described as such. Any software used to perform simulation runs (computational Measurements) and to generate Raw Data, it is considered an Instrument and should be described as such."@en .

mdmc:SampleComponent a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:SampleComponentFabrication ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isSampleComponentOf ;
            owl:someValuesFrom mdmc:Sample ],
        prov:Entity ;
    skos:definition "Identifiable entity (typically a piece of material) with distinctive properties (structural, chemical, dimensional, functional and others), which is fabricated during the Sample Component Fabrication and is used during the Sample Preparation to produce a Sample. It may include substrate, layer, mask, evaporation material, coating, and the molecule itself."@en .

mdmc:Sample a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasGeneratedBy ;
            owl:someValuesFrom mdmc:SamplePreparation ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isCarriedBy ;
            owl:someValuesFrom mdmc:SampleCarrier ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:hasSampleComponent ;
            owl:someValuesFrom mdmc:SampleComponent ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasDerivedFrom ;
            owl:someValuesFrom mdmc:Sample ],
        prov:Entity ;
    skos:definition "Identifiable entity (typically a piece of material) with distinctive properties (structural, chemical, dimensional, functional and others), composed by one or more Sample Components, exposed to the Instrument during a Measurement. Sample may be held by a Sample Holder and/or carried by a Sample Carrier during the Measurement. Sample may also stand for a model, configuration or input (or any combination of them) of a simulation run (computational Measurement)."@en .

ex:STRAS_Research_Group a mdmc:ResearchUser .

mdmc:DataAnalysisLifeCycle a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchSoftware ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:ResearchData ],
        [ a owl:Restriction ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:someValuesFrom mdmc:ResearchUser ],
        prov:Activity ;
    skos:definition "Set of processes carried out by one or more Research Users, who systematically perform actions and apply methods, e.g., statistical and/or logical techniques, on Research Data in order to produce synthesized knowledge to, e.g., detect pattern, determine relationships, develop explanations, test hypotheses, prove theories, suggest the Conclusions of the Study. Data Analysis Lifecycle includes (but is not limited to): Data Processing, Data Analysis, Data Interpretation. These processes may consist of different steps, may be iterative and may be combined in chains or workflows. Data Analysis Lifecycle may be performed using Research Software."@en .

mdmc:Measurement a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:hasMeasurementTechnique ;
            owl:someValuesFrom mdmc:MeasurementTechnique ],
        [ a owl:Restriction ;
            owl:onClass mdmc:Instrument ;
            owl:onProperty prov:wasAssociatedWith ;
            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ],
        [ a owl:Restriction ;
            owl:onProperty mdmc:isMeasurementOf ;
            owl:someValuesFrom mdmc:Experiment ],
        [ a owl:Restriction ;
            owl:onProperty prov:used ;
            owl:someValuesFrom mdmc:Sample ],
        prov:Activity ;
    skos:definition "Identifiable and reproducible activity, performed by one or more Research Users, who generate a single self-consistent unit of Raw Data about a Sample or a set of them using an Instrument under constant or varying controlled conditions, depending on the particular research context. Measurement is specific to Instrument: an investigation on the same Sample conducted using a different Instrument implies a different Measurement. A Measurement may also be performed during the Sample Component Synthesis and/or the Sample Preparation, e.g., to characterize the resulting Sample or Sample Component(s). A computational Measurement may be a part of a simulation (computational Experiment), e.g., a simulation run using a particular model, configuration or input(s)."@en .

prov:Activity a owl:Class .

prov:used a owl:ObjectProperty .

mdmc:Equipment a owl:Class ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:atLocation ;
            owl:someValuesFrom mdmc:Laboratory ],
        prov:Agent ;
    skos:definition "Any kind of item, device, machine or other tool (also virtual) used by one or more Research Users to perform one or more Sample Component Syntheses, Sample Preparations and/or Measurements. Usually, the Equipment is located in a Laboratory hosted by an Institution. According to this definition, an Instrument is a particular type of Equipment."@en .

prov:Entity a owl:Class .

prov:wasGeneratedBy a owl:ObjectProperty .

mdmc:ResearchData a owl:Class ;
    rdfs:comment "Research data is the data in the research context."@en ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty mdmc:hasMetadata ;
            owl:someValuesFrom mdmc:Metadata ],
        prov:Entity ;
    skos:definition "Data collected, created or examined by one or more Research Users to be analysed or considered as a basis for reasoning, discussion or calculation in a research context, with the purpose of generating, verifying and validating original scientific claims that support the answer to some specific research question (i.e., Conclusions). Examples of Research Data include statistics, output of Experiments and/or Measurements, observations resulting from fieldwork, survey results, recordings and images. According to this definition, Raw Data, Processed Data, Analysed Data and Reference Data are particular types of Research Data. Research Data is typically in the form of a data file, but it may potentially be a data stream or any other form of data which is relevant in a particular data management context. Research Data may be stored in a Data Collaboration Platform and/or in a Data Repository. Research Data may be part of a Dataset."@en .

mdmc:ResearchUser a owl:Class ;
    rdfs:subClassOf prov:Agent ;
    skos:definition "Person, usually member of a Project, who conducts any part of the Experiment(s), or performs any of the steps of the Data Analysis Lifecycle during the course of one or more Studies, in order to collect and/or analyse Research Data, or is interested in reusing Research Data collected by a third party (e.g., Reference Data) with the final aim to extract insights that support the answer to some specific research question (i.e., Conclusions). Research Users may be assigned with a role (e.g., principal investigator, Instrument scientist, Project member)."@en .

prov:wasAssociatedWith a owl:ObjectProperty .

