prima-core:Project a owl:Class ;
    rdfs:comment "Project is an enterprise (potentially individual but typically collaborative) of one or more research users, planned to perform one or more studies." ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:onProperty prov:wasAttributedTo ;
            owl:someValuesFrom prima-core:ResearchUser ],
        [ a owl:Restriction ;
            owl:onProperty prima-core:hasStudy ;
            owl:someValuesFrom prima-core:Study ],
        prov:Entity ;
    prima-core:MDMCNEPGlossaryDefinition "Project is an enterprise (potentially individual but typically collaborative) of one or more research users, planned to perform one or more studies."@en .

